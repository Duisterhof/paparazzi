<!DOCTYPE settings SYSTEM "../settings.dtd">

<settings>
  <dl_settings>
    <dl_settings NAME="Delfly_Utils">
      <dl_setting var="LEDS_switch" min="0" step="1" max="1" module="delfly_utils/delfly_utils" shortname="LEDS" param="DELFLY_UTILS_LEDS_SWITCH"/>
      <dl_setting var="SRVO_kill" min="0" step="1" max="1" module="delfly_utils/delfly_utils" shortname="SRVO_kill" param="DELFLY_UTILS_SRVO_KILL"/>
    <!--/dl_settings>
    <dl_settings NAME="PIV_Utils"-->
      <dl_setting var="delfly_wp_piv" min="1" step="1" max="3" values="WP_PIV1|WP_PIV2|WP_PIV3" module="delfly_utils/delfly_utils" shortname="WP" handler="piv_select_wp"/>
      <dl_setting var="piv_wp_north*100" min="-50" step="1" max="0" module="delfly_utils/delfly_utils" shortname="wp_north" handler="piv_set_wp_north"/>
      <dl_setting var="piv_east*100" min="-15" step="0.1" max="15" module="delfly_utils/delfly_utils" shortname="east" handler="piv_set_east"/>
      <dl_setting var="piv_height*100" min="100" step="1" max="150" module="delfly_utils/delfly_utils" shortname="height" handler="piv_set_height"/>
      <dl_setting var="piv_wp" min="1" step="1" max="0" module="delfly_utils/delfly_utils" shortname="WP_PIV"/> 
    </dl_settings>  
  </dl_settings>
</settings>
